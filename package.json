{
  "name": "eazylab-inventory-reservation",
  "version": "1.0.0",
  "description": "Inventory Reservation Plugin for Vendure - EazyLab Mini Test",
  "author": "Your Name",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "migration:generate": "ts-node node_modules/typeorm/cli.js migration:generate -d src/migration-data-source.ts",
    "migration:run": "ts-node node_modules/typeorm/cli.js migration:run -d src/migration-data-source.ts",
    "migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert -d src/migration-data-source.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@vendure/admin-ui": "^2.1.0",
    "@vendure/admin-ui-plugin": "^2.1.0",
    "@vendure/core": "^2.1.0",
    "dotenv": "^16.6.1",
    "ioredis": "^5.3.0",
    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "redlock": "^5.0.0-beta.2",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vendure/testing": "^2.1.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/*.spec.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.spec.ts",
      "!src/examples/**"
    ]
  }
}
